{
	"manifest_version": 2,
	"name": "hsDebug Extension",
	"version": "2.0",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["jquery-3.2.1.min.js", "background.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "icon2.png"
	},
	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	},
	"permissions": [
		"tabs"
	]
}